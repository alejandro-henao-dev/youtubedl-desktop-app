{
  "name": "client",
  "version": "1.0.0",
  "author": {
    "name": "RAT"
  },
  "description": "RAT Electron App",
  "main": "electron/index.js",
  "scripts": {
    "serve:client": "ng serve",
    "dev:angular": "ng build  --base-href ./ --watch",
    "dev:electron": "electron .",
    "dev": "concurrently \"npm run dev:angular\" \"npm run dev:electron\"",
    "start": "ng build --base-href ./ && npm run dev:electron",
    "prod:angular": "ng build  --base-href ./ --prod",
    "build": "npm run prod:angular && electron-builder build",
    "build:electron": "electron-builder build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/forms": "~10.1.5",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "concurrently": "^5.3.0",
    "electron-log": "^4.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "ytdl-core": "^4.0.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.6",
    "@angular/cli": "~10.1.6",
    "@angular/compiler-cli": "~10.1.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^10.1.4",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
